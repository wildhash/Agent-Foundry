# Application Settings
APP_NAME=Agent Foundry
DEBUG=False

# API Keys (Get your keys from respective services)
FASTINO_API_KEY=fastino_dev_key
FREEPIK_API_KEY=freepik_dev_key
FRONTEGG_API_KEY=frontegg_dev_key
AIRIA_API_KEY=airia_dev_key

# CORS Origins (comma-separated)
CORS_ORIGINS=http://localhost:3000,http://localhost:8000

# Database URL
# SQLite (development)
DATABASE_URL=sqlite:///./agent_foundry.db
# PostgreSQL (production)
# DATABASE_URL=postgresql://user:password@localhost:5432/agentfoundry

# Redis URL
REDIS_URL=redis://localhost:6379

# Agent Configuration
MAX_REFLEXION_LOOPS=5
PERFORMANCE_THRESHOLD=0.75
EVOLUTION_THRESHOLD=0.85

# Fastino TLM Settings (99x faster inference)
FASTINO_INFERENCE_SPEED_MULTIPLIER=99
FASTINO_BATCH_SIZE=32
FASTINO_MAX_TOKENS=2048

# LiquidMetal Raindrop Settings (self-healing code)
RAINDROP_AUTO_HEAL=True
RAINDROP_MAX_ATTEMPTS=3
RAINDROP_HEAL_TIMEOUT=30
